{
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": "18"
  },
  "scripts": {
    "setup": "panda codegen && remix-routes && convex codegen",
    "dev": "pnpm run --parallel /^dev:/",
    "dev:remix": "remix dev",
    "dev:routes": "remix-routes --watch",
    "dev:convex": "convex dev",
    "build": "remix build",
    "start": "remix-serve build",
    "lint": "pnpm run --parallel /^lint:/",
    "lint:tsc": "tsc -b",
    "lint:eslint": "eslint .",
    "lint:knip": "knip",
    "format": "pnpm run --sequential /^format:/",
    "format:eslint": "eslint . --fix",
    "format:prettier": "prettier --write ."
  },
  "dependencies": {
    "@fontsource-variable/spline-sans": "^5.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@remix-run/css-bundle": "^1.19.1",
    "@remix-run/node": "^1.19.1",
    "@remix-run/react": "^1.19.1",
    "@remix-run/serve": "^1.19.1",
    "convex": "^1.0.2",
    "isbot": "^3.6.13",
    "lucide-react": "^0.263.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-virtuoso": "^4.4.2",
    "remix-routes": "^1.4.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.3.3",
    "@pandacss/dev": "^0.7.0",
    "@pandacss/preset-panda": "^0.7.0",
    "@remix-run/dev": "^1.19.1",
    "@remix-run/eslint-config": "^1.19.1",
    "@types/node": "^20.4.4",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.45.0",
    "knip": "^2.16.1",
    "prettier": "^3.0.0",
    "typescript": "~5.1.6"
  }
}
