{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "prepare": "cpy \"node_modules/@fontsource/rubik/**/{variable.css,*-variable-wghtOnly-normal.woff2}\" \"public/fonts/rubik\"",
    "dev": "run-p --print-label --race dev:*",
    "dev:remix": "remix dev",
    "dev:tailwind": "pnpm tailwind --watch",
    "build": "pnpm tailwind --minify && remix build",
    "typecheck": "tsc -b",
    "tailwind": "tailwindcss -i ./styles/app.css -o app/styles/app.css"
  },
  "dependencies": {
    "@fontsource/rubik": "^4.5.13",
    "@remix-run/node": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "@remix-run/vercel": "^1.9.0",
    "@vercel/node": "^2.8.2",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "lucide-react": "0.105.0-alpha.4",
    "mime-types": "^2.1.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.9.0",
    "@remix-run/eslint-config": "^1.9.0",
    "@remix-run/serve": "^1.9.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "autoprefixer": "^10.4.13",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.30.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  }
