{
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "prisma generate",
    "clean": "del-cli build public/build .cache",
    "dev": "pnpm clean && cross-env NODE_ENV=development run-p --race dev:*",
    "dev:remix": "remix dev --sourcemap",
    "dev:tailwind": "pnpm run tailwind --watch",
    "dev:routes": "pnpm run routes --watch",
    "build": "pnpm clean && tsc --noEmit && pnpm run tailwind --minify && pnpm run routes && remix build",
    "start": "cross-env NODE_ENV=production remix-serve build",
    "lint": "eslint --ext js,ts,tsx,astro .",
    "lint-fix": "pnpm lint --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "tailwind": "tailwindcss -i styles/styles.css -o generated/styles.css -c tailwind.config.cjs",
    "routes": "routes-gen --driver @routes-gen/remix",
    "env-check": "tsx app/core/env.server.ts",
    "unused-deps": "depcheck"
  },
  "dependencies": {
    "@fontsource/rubik": "^4.5.11",
    "@prisma/client": "^4.6.1",
    "@remix-run/node": "^1.7.6",
    "@remix-run/react": "^1.7.6",
    "@remix-run/serve": "^1.7.6",
    "@supabase/supabase-js": "^2.1.0",
    "ariakit": "2.0.0-next.41",
    "chalk": "^5.1.2",
    "clsx": "^1.2.1",
    "cuid": "^2.1.8",
    "dotenv": "^16.0.3",
    "execa": "^6.1.0",
    "immer": "^9.0.16",
    "lucide-react": "^0.101.0",
    "node-cron": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-expanding-textarea": "^2.3.6",
    "react-mosaic-component": "^5.3.0",
    "react-virtuoso": "^3.1.4",
    "reconnecting-eventsource": "^1.6.2",
    "routes-gen": "^0.5.2",
    "use-sync-external-store": "^1.2.0",
    "zod": "^3.19.1",
    "zod-form-data": "^1.2.4"
  },
  "devDependencies": {
    "@itsmapleleaf/configs": "^1.1.5",
    "@remix-run/dev": "^1.7.6",
    "@routes-gen/remix": "^0.3.5",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.9",
    "@types/node-cron": "^3.0.6",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.28.0",
    "eslint-plugin-tailwindcss": "^3.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prisma": "^4.6.1",
    "prop-types": "^15.8.1",
    "remix-flat-routes": "^0.4.8",
    "tailwindcss": "^3.2.4",
    "tsx": "^3.12.1",
    "typescript": "4.9.3"
  },
  "prettier": "@itsmapleleaf/configs/prettier"
}
