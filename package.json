{
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": "18"
  },
  "scripts": {
    "prepare": "prisma generate",
    "clean": "del-cli build public/build .cache api",
    "dev": "cross-env NODE_ENV=development run-p --race dev:*",
    "dev:remix": "remix dev",
    "dev:server": "wait-on file:build && cross-env NODE_ENV=development nodemon -w build server.dev.mjs",
    "dev:routes": "pnpm run routes --watch",
    "build": "pnpm run routes && remix build",
    "start": "cross-env NODE_ENV=production remix-serve build",
    "lint": "eslint --ext js,ts,tsx,astro .",
    "lint-fix": "pnpm lint --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "routes": "routes-gen --driver @routes-gen/remix",
    "env-check": "tsx app/core/env.server.ts",
    "unused-deps": "depcheck"
  },
  "dependencies": {
    "@fontsource/rubik": "^4.5.14",
    "@formkit/auto-animate": "1.0.0-beta.6",
    "@liveblocks/client": "^1.0.0",
    "@liveblocks/node": "^1.0.0",
    "@liveblocks/react": "^1.0.0",
    "@prisma/client": "^4.10.1",
    "@radix-ui/colors": "^0.1.8",
    "@remix-run/node": "0.0.0-experimental-0ec995c53",
    "@remix-run/react": "0.0.0-experimental-0ec995c53",
    "@remix-run/vercel": "0.0.0-experimental-0ec995c53",
    "@vercel/node": "^2.9.4",
    "ariakit": "2.0.0-next.43",
    "clsx": "^1.2.1",
    "cuid": "^3.0.0",
    "inngest": "^1.1.0",
    "lucide-react": "^0.114.0",
    "pretty-bytes": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-expanding-textarea": "^2.3.6",
    "react-mosaic-component": "^5.3.0",
    "react-virtuoso": "^4.1.0",
    "routes-gen": "^0.6.0",
    "zod": "^3.20.6",
    "zod-form-data": "^1.3.1"
  },
  "devDependencies": {
    "@itsmapleleaf/configs": "^1.1.5",
    "@remix-run/dev": "0.0.0-experimental-0ec995c53",
    "@remix-run/express": "0.0.0-experimental-0ec995c53",
    "@remix-run/serve": "0.0.0-experimental-0ec995c53",
    "@routes-gen/remix": "^0.3.6",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tailwindcss/container-queries": "^0.1.0",
    "@types/eslint": "^8.21.1",
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.34.0",
    "eslint-plugin-tailwindcss": "^3.8.3",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "prisma": "^4.10.1",
    "react-refresh": "^0.14.0",
    "tailwindcss": "^3.2.6",
    "tsx": "^3.12.3",
    "typescript": "4.9.5",
    "wait-on": "^7.0.1"
  },
  "prettier": "@itsmapleleaf/configs/prettier",
  "pnpm": {
    "patchedDependencies": {
      "@remix-run/dev@0.0.0-experimental-0ec995c53": "patches/@remix-run__dev@0.0.0-experimental-0ec995c53.patch"
