{
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": "18"
  },
  "scripts": {
    "prepare": "prisma generate",
    "clean": "del-cli build public/build .cache api",
    "dev": "pnpm clean && cross-env NODE_ENV=development run-p --race dev:*",
    "dev:remix": "remix dev --sourcemap",
    "dev:routes": "pnpm run routes --watch",
    "build": "pnpm run routes && remix build",
    "start": "cross-env NODE_ENV=production remix-serve build",
    "lint": "eslint --ext js,ts,tsx,astro .",
    "lint-fix": "pnpm lint --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "routes": "routes-gen --driver @routes-gen/remix",
    "env-check": "tsx app/core/env.server.ts",
    "unused-deps": "depcheck"
  },
  "dependencies": {
    "@fontsource/rubik": "^4.5.14",
    "@formkit/auto-animate": "1.0.0-beta.6",
    "@liveblocks/client": "^1.0.0",
    "@liveblocks/node": "^1.0.0",
    "@liveblocks/react": "^1.0.0",
    "@prisma/client": "^4.10.1",
    "@remix-run/node": "^1.13.0",
    "@remix-run/react": "^1.13.0",
    "@remix-run/vercel": "^1.13.0",
    "@vercel/node": "^2.9.4",
    "ariakit": "2.0.0-next.43",
    "clsx": "^1.2.1",
    "cuid": "^3.0.0",
    "inngest": "^1.1.0",
    "lucide-react": "^0.114.0",
    "pretty-bytes": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-expanding-textarea": "^2.3.6",
    "react-mosaic-component": "^5.3.0",
    "react-virtuoso": "^4.1.0",
    "routes-gen": "^0.6.0",
    "zod": "^3.20.6",
    "zod-form-data": "^1.3.1"
  },
  "devDependencies": {
    "@itsmapleleaf/configs": "^1.1.5",
    "@remix-run/dev": "^1.13.0",
    "@remix-run/serve": "^1.13.0",
    "@routes-gen/remix": "^0.3.6",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/eslint": "^8.21.1",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.34.0",
    "eslint-plugin-tailwindcss": "^3.8.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "prisma": "^4.10.1",
    "tailwindcss": "^3.2.6",
    "tsx": "^3.12.3",
    "typescript": "4.9.5"
  },
  "prettier": "@itsmapleleaf/configs/prettier"
}
