{
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": "^16"
  },
  "scripts": {
    "prepare": "prisma generate",
    "dev": "run-p --race dev:*",
    "dev:remix": "remix dev --sourcemap",
    "dev:tailwind": "pnpm run tailwind --watch",
    "dev:routes": "pnpm run routes --watch",
    "build": "tsc --noEmit && pnpm run tailwind --minify && pnpm run routes && remix build",
    "start": "remix-serve build",
    "lint": "eslint --ext js,ts,tsx,astro .",
    "lint-fix": "pnpm lint --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "tailwind": "tailwindcss -i app/ui/styles.css -o app/generated/styles.css -c tailwind.config.cjs",
    "routes": "routes-gen --driver @routes-gen/remix",
    "env-check": "tsx app/core/env.server.ts"
  },
  "dependencies": {
    "@fontsource/rubik": "^4.5.11",
    "@prisma/client": "^4.5.0",
    "@remix-run/node": "^1.7.3",
    "@remix-run/react": "^1.7.3",
    "@remix-run/serve": "^1.7.3",
    "@remix-run/server-runtime": "^1.7.3",
    "ariakit": "2.0.0-next.41",
    "clsx": "^1.2.1",
    "cuid": "^2.1.8",
    "lucide-react": "^0.92.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-expanding-textarea": "^2.3.6",
    "react-mosaic-component": "^5.3.0",
    "react-use-measure": "^2.1.1",
    "react-virtuoso": "^3.1.1",
    "routes-gen": "^0.5.2",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@itsmapleleaf/configs": "^1.1.5",
    "@remix-run/dev": "^1.7.3",
    "@routes-gen/remix": "^0.3.5",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/eslint": "^8.4.7",
    "@types/node": "^18.11.3",
    "@types/prettier": "^2.7.1",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.25.0",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prisma": "^4.5.0",
    "prop-types": "^15.8.1",
    "remix-flat-routes": "^0.4.4",
    "tailwindcss": "^3.2.0",
    "tsx": "^3.10.4",
    "typescript": "4.9.1-beta"
  },
  "prettier": "@itsmapleleaf/configs/prettier"
}
